{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "react/prop-types": "off",
        "no-class-assign": "off",
        "import/no-mutable-exports": "off",
        "react/no-did-update-set-state": "off",
        "react/no-did-mount-set-state": "off",
        "no-underscore-dangle": "off"
    }
}